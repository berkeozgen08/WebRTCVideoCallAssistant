{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/WebRTCVideoCallAssistant.Server.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "publish",
			"command": "dotnet",
			"type": "process",
			"args": [
				"publish",
				"${workspaceFolder}/WebRTCVideoCallAssistant.Server.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "watch",
			"command": "dotnet",
			"type": "process",
			"args": [
				"watch",
				"run",
				"--project",
				"${workspaceFolder}/WebRTCVideoCallAssistant.Server.csproj"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "ef",
			"command": "dotnet",
			"type": "process",
			"args": [
				"ef",
				"dbcontext",
				"scaffold",
				"--force",
				"Name=default",
				"Microsoft.EntityFrameworkCore.SqlServer",
				"-o", "Models",
				"-c", "AppDbContext"
			],
			"problemMatcher": "$msCompile"
		}
	]
}